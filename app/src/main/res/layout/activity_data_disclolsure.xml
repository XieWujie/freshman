<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.activity.DataDisclosureActivity">

    <RelativeLayout
        android:id="@+id/necessary_app_bar"
        android:layout_width="match_parent"
        android:layout_height="33dp"
        android:background="#ff5CACFC">
        <ImageView
            android:id="@+id/data_d_back"
            android:layout_width="10dp"
            android:layout_height="20dp"
            android:src="@drawable/freshman_back"
            android:layout_centerVertical="true"
            android:layout_marginLeft="16dp"/>
        <TextView
            android:layout_width="73dp"
            android:layout_height="18dp"
            android:textSize="15sp"
            android:gravity="center"
            android:text="数据揭秘"
            android:textColor="#ffFEFEFF"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            />
    </RelativeLayout>
    <android.support.v7.widget.RecyclerView
        android:id="@+id/data_d_recycler_view"
        android:layout_marginTop="33dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</RelativeLayout>